{
  "version": 2,
  "name": "plainV2",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "content/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "content/images/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
